	option ip6deprecated '1'
	option proto 'static'

config interface 'gluon_bat0'
	option gw_mode 'client'
	option proto 'gluon_bat0'

config interface 'bat0'
	option multicast_router '2'
	option ifname 'bat0'
	option auto '1'
	option macaddr 'f8:1a:67:5a:5d:e8'
	option learning '1'
	option proto 'none'

