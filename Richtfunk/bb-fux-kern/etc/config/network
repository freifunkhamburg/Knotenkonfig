
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config switch
	option reset '1'
	option enable_vlan '1'
	option name 'switch0'

config switch_vlan
	option vlan '1'
	option ports '0t 3 4 5'
	option device 'switch0'

